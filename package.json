{
	"name": "evansstepanov",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "npm run download-images:dev && vite dev",
		"download-images": "NODE_OPTIONS='--import=tsx' node ./scripts/download-build-images.ts",
		"download-images:dev": "NODE_OPTIONS='--import=tsx' node --env-file=.env ./scripts/download-build-images.ts",
		"build": "npm run download-images && vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^5.0.0",
		"@sveltejs/adapter-static": "^3.0.0",
		"@sveltejs/kit": "^2.20.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@typescript-eslint/eslint-plugin": "^8.27.0",
		"@typescript-eslint/parser": "^8.27.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^9.23.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-svelte": "^3.3.3",
		"postcss": "^8.4.24",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.25.0",
		"svelte-check": "^4.1.5",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.4.1",
		"tsx": "^4.16.5",
		"typescript": "^5.0.0",
		"vite": "^6.2.0",
		"vitest": "^1.0.0"
	},
	"type": "module",
	"dependencies": {
		"@icons-pack/svelte-simple-icons": "^5.0.0",
		"@lucide/svelte": "^0.483.0",
		"@notionhq/client": "^2.2.5",
		"fathom-client": "^3.6.0",
		"simple-icons": "^14.11.1"
	}
}
